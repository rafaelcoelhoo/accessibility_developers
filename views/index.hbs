<section class="index-page">
    {{#each content}}
        <h2 class="mt-5">{{title}}</h2>
        <ul class="ms-5">
            {{#each options}}
                {{#if hasMultipleLinks}}
                    <li class="border-bottom border-dark list-links"><a href="/?id={{id}}">{{label}}</a></li>
                {{else}}
                    <li class="border-bottom border-dark list-links"><a href="{{link}}">{{label}}</a></li>
                {{/if}}
            {{/each}}
        </ul>
    {{/each}}
</section>
